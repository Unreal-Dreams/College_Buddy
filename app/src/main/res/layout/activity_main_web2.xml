        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="OLA"/>
    <Button
        android:id="@+id/redbus"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="REDBUS"/>
    <Button
        android:id="@+id/udemy"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="UDEMY"/>
    <Button
        android:id="@+id/twitter"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
