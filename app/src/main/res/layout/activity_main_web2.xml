    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".WebServices.MainActivityWeb">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:id="@+id/zomato"
        android:text="Zomato"/>

    <Button
